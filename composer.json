{
    "name": "grafite/database",
    "description": "A handy set of commands for MySQL databases",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "spatie/db-dumper": "^2.15|^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^5.0|^6.0",
        "orchestra/testbench": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\Database\\GrafiteDatabaseProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
